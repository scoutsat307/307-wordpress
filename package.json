{
  "name": "scoutsat307",
  "version": "1.0.0",
  "description": "Wordpress theme for Troop 307",
  "author": "Michael Thomas <michaelhthomas@outlook.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "parcel:watch-js": "parcel watch static/_js/* --no-source-maps --out-dir static/dist/",
    "parcel:build-js": "parcel build static/_js/* --no-source-maps --out-dir static/dist/",
    "parcel:watch-css": "parcel watch static/_sass/main.scss --no-source-maps --no-hmr --out-dir static/dist/",
    "parcel:build-css": "parcel build static/_sass/main.scss --no-source-maps --out-dir static/dist/",
    "serve": "run-p parcel:watch-js parcel:build-css",
    "build": "run-s parcel:build-js parcel:build-css"
  },
  "devDependencies": {
    "bulma": "^0.9.1",
    "bulma-helpers": "^0.3.12",
    "bulmaswatch": "^0.8.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "sass": "^1.32.0"
  }
}
